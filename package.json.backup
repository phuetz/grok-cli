{
  "name": "@phuetz/grok-cli",
  "version": "1.0.0",
  "description": "An open-source AI agent that brings the power of Grok directly into your terminal.",
  "author": "Patrice Huetz <ton-email@example.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/phuetz/grok-cli.git"
  },
  "bugs": {
    "url": "https://github.com/phuetz/grok-cli/issues"
  },
  "homepage": "https://github.com/phuetz/grok-cli#readme",
  "type": "module",
  "main": "dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "bin": {
    "grok": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "build:bun": "bun run tsc",
    "build:watch": "tsc --watch",
    "clean": "rm -rf dist coverage .nyc_output *.tsbuildinfo",
    "dev": "bun run src/index.ts",
    "dev:node": "tsx src/index.ts",
    "start": "node dist/index.js",
    "start:bun": "bun run dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,md}\"",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch",
    "validate": "npm run lint && npm run typecheck && npm test",
    "install:bun": "bun install",
    "prepare": "npm run build"
  },
  "keywords": [
    "cli",
    "agent",
    "text-editor",
    "grok",
    "ai",
    "coding-assistant",
    "terminal",
    "developer-tools",
    "mcp",
    "hooks"
  ],
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.24.3",
    "@vscode/ripgrep": "^1.17.0",
    "adm-zip": "^0.5.16",
    "axios": "^1.13.2",
    "better-sqlite3": "^11.10.0",
    "chalk": "^5.6.2",
    "cli-highlight": "^2.1.11",
    "commander": "^12.1.0",
    "diff-match-patch": "^1.0.5",
    "dotenv": "^16.6.1",
    "fast-glob": "^3.3.3",
    "form-data": "^4.0.5",
    "fs-extra": "^11.3.2",
    "ignore": "^5.3.2",
    "ink": "^4.4.1",
    "marked": "^15.0.12",
    "marked-terminal": "^7.3.0",
    "openai": "^5.23.2",
    "react": "^18.3.1",
    "semver": "^7.7.3",
    "tiktoken": "^1.0.22",
    "zod": "^4.1.13"
  },
  "optionalDependencies": {
    "@mlc-ai/web-llm": "^0.2.80",
    "@xenova/transformers": "^2.17.2",
    "node-llama-cpp": "^3.14.5",
    "node-pty": "^1.0.0"
  },
  "devDependencies": {
    "@types/adm-zip": "^0.5.7",
    "@types/better-sqlite3": "^7.6.13",
    "@types/diff-match-patch": "^1.0.36",
    "@types/fs-extra": "^11.0.4",
    "@types/glob": "^8.1.0",
    "@types/jest": "^29.5.14",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.19.26",
    "@types/react": "^18.3.27",
    "@types/semver": "^7.7.1",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^8.49.0",
    "@typescript-eslint/parser": "^8.49.0",
    "eslint": "^9.39.1",
    "jest": "^29.7.0",
    "prettier": "^3.7.4",
    "ts-jest": "^29.4.6",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vscode-languageserver": "^9.0.1",
    "vscode-languageserver-textdocument": "^1.0.12"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "preferGlobal": true
}
